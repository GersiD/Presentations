%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\setbeamertemplate{block begin}{
  \vskip.75ex
  \begin{beamercolorbox}[leftskip=0cm,sep=0ex,colsep*=0.7cm]{block title}
  \begin{beamercolorbox}[leftskip=0cm,sep=0ex,colsep*=0ex,ht=2.25ex,dp=1ex]{block title}
    \usebeamerfont*{block title}\insertblocktitle
  \end{beamercolorbox}
  
  \usebeamerfont{block body}
  \begin{beamercolorbox}[colsep=0cm,sep=0cm,colsep*=0.5cm,vmode]{block body}%
  }
  \setbeamertemplate{block end}{
  \end{beamercolorbox}\vspace{-0.5cm}\end{beamercolorbox}
}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\setbeamertemplate{headline}{  
  \leavevmode

  \begin{beamercolorbox}[wd=\paperwidth]{headline}
    \begin{columns}[T]
    \begin{column}{.01\paperwidth}
    \end{column}
                    
    \begin{column}{0.96\paperwidth}
            \vskip6ex
%            \raggedright
            \centering
            \usebeamercolor{title in headline}{\color{fg}\textbf{\huge{\inserttitle}}\\[2ex]} 
          \end{column} \\
      \begin{column}{.01\paperwidth}
      \end{column}
    \end{columns}
  \end{beamercolorbox}
  
    \begin{beamercolorbox}[wd=\paperwidth]{headline}
  	\begin{columns}[T]
  		\begin{column}{0.85\paperwidth}
                \vskip1.5ex
                \centering
\usebeamercolor{author in headline}{\color{fg}\large{\insertauthor}\\[1ex]}
\usebeamercolor{institute in headline}{\color{fg}\normalsize{\insertinstitute}\\[1ex]}     
                \vskip2ex
  		\end{column} 
  	\end{columns}
  \end{beamercolorbox}
  
  \begin{beamercolorbox}[wd=\paperwidth]{lower separation line head}
    \rule{0pt}{2pt}
  \end{beamercolorbox}
}

